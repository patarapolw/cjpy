{
  "name": "cnpy",
  "version": "0.4.0",
  "description": "Pinyin SRS that repeats until everything right",
  "main": "index.js",
  "scripts": {
    "start": "pnpm run py app.py",
    "dev": "pnpm run py app.py --debug",
    "vocab": "pnpm run py app.py --vocab",
    "build": "pnpm run py build.py %npm_package_version%",
    "build:nightly": "pnpm run py build.py %npm_package_version%-nightly",
    "build:preview": "pnpm run py build.py %npm_package_version%-preview",
    "py:requirements": "pipenv requirements > requirements.txt",
    "py:install": "pipenv install",
    "py": "pipenv run py"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/showdown": "^2.0.6"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}
